{
  "presets": [
    "es2015",
    "react",
    "babel-preset-env",
    "stage-1"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    },
    "transform-function-bind",
    "transform-deceorators-legacy"
    ]
  ]
}
