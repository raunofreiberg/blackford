  "main": "./server/init.js",
  "scripts": {
    "dev": "concurrently \"NODE_ENV=development npm run client\" \"NODE_ENV=development npm run server\"",
    "client": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress --colors",
    "server": "./node_modules/.bin/nodemon ./server/init.js",
    "build": "webpack -p --config webpack/webpack.prod.js --progress --profile --color",
    "start": "NODE_ENV=production npm run server",
    "prod": "concurrently \"npm run build\" \"npm run migrate\" \"npm run start\"",
    "migrate": "knex migrate:latest",
    "lint": "eslint client/ --ext .js --ext .jsx && eslint server/ --ext .js",
    "test": "jest",
    "cover": "istanbul cover jest --no-default-excludes",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
